{
  "name": "kcms-bootstrap",
  "version": "0.1.0",
  "license": "UNLICENCED",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf cdk.out ts-build",
    "synth": "yarn build; yarn cdk synth",
    "deploy": "yarn cdk deploy --require-approval never",
    "bootstrap": "yarn cdk bootstrap --app '' --qualifier keycore --toolkit-stack-name KeyCoreCDKToolkit --cloudformation-execution-policies arn:aws:iam::aws:policy/AdministratorAccess",
    "publish": "ls -la",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.14.6",
    "aws-cdk": "^2.67.0",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.67.0",
    "constructs": "^10.1.270",
    "source-map-support": "^0.5.21"
  }
}
